<?xml version="1.0"?>
<robot name="walk_bot">
  <!-- Links -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.00"/>
      <inertia
        ixx="0.00167"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00167"
        iyz="0.0"
        izz="0.00167"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>

  <link name="l_hip1">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
  </inertial>
  </link>
  <link name="r_hip1">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="l_hip2">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="r_hip2">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="l_ankle1">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="r_ankle1">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="l_ankle2">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="r_ankle2">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="l_ankle3">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="r_ankle3">
    <inertial>
      <mass value="0.001"/>
      <inertia
        ixx="0.00001"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00001"
        iyz="0.0"
        izz="0.00001"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>

  <link name="l_sole">
    <visual>
      <geometry>
        <box size="0.15 0.05 0.02"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.05 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <inertia
        ixx="0.000725"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00625"
        iyz="0.0"
        izz="0.005725"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>
  <link name="r_sole">
    <visual>
      <geometry>
        <box size="0.15 0.05 0.02"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.05 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <inertia
        ixx="0.000725"
        ixy="0.0"
        ixz="0.0"
        iyy="0.00625"
        iyz="0.0"
        izz="0.005725"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    </inertial>
  </link>

  <!-- Joints -->
  <joint name="l_hip_pitch" type="revolute">
    <parent link="base_link"/>
    <child link="l_hip1"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="r_hip_pitch" type="revolute">
    <parent link="base_link"/>
    <child link="r_hip1"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="l_hip_roll" type="revolute">
    <parent link="l_hip1"/>
    <child link="l_hip2"/>
    <axis xyz="1 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="r_hip_roll" type="revolute">
    <parent link="r_hip1"/>
    <child link="r_hip2"/>
    <axis xyz="1 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="l_leg_extension" type="prismatic">
    <parent link="l_hip2"/>
    <child link="l_ankle1"/>
    <limit effort="20.0" lower="0.2" upper="0.4" velocity="2.0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <joint name="r_leg_extension" type="prismatic">
    <parent link="r_hip2"/>
    <child link="r_ankle1"/>
    <limit effort="20.0" lower="0.2" upper="0.4" velocity="2.0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <joint name="l_ankle_yaw" type="revolute">
    <parent link="l_ankle1"/>
    <child link="l_ankle2"/>
    <axis xyz="0 0 1"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="r_ankle_yaw" type="revolute">
    <parent link="r_ankle1"/>
    <child link="r_ankle2"/>
    <axis xyz="0 0 1"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="l_ankle_pitch" type="revolute">
    <parent link="l_ankle2"/>
    <child link="l_ankle3"/>
    <axis xyz="0 1 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="r_ankle_pitch" type="revolute">
    <parent link="r_ankle2"/>
    <child link="r_ankle3"/>
    <axis xyz="0 1 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="l_ankle_roll" type="revolute">
    <parent link="l_ankle3"/>
    <child link="l_sole"/>
    <axis xyz="1 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <joint name="r_ankle_roll" type="revolute">
    <parent link="r_ankle3"/>
    <child link="r_sole"/>
    <axis xyz="1 0 0"/>
    <limit effort="20.0" lower="-3.14" upper="3.14" velocity="2.0"/>
  </joint>

  <gazebo reference="l_sole">
    <sensor name='sensor_contact' type='contact'>
      <contact>
        <collision>l_sole_collision</collision>
        <topic>/contact_example</topic>
      </contact>
      <always_on>1</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
    </sensor>
  </gazebo>

  <gazebo>
    <plugin
      filename="ignition-gazebo-contact-system"
      name="ignition::gazebo::systems::Contact"/>

    <plugin
        filename="libignition-gazebo-joint-state-publisher-system.so"
        name="ignition::gazebo::systems::JointStatePublisher">
      <topic>joint_states</topic>
      <joint_name>l_hip_pitch</joint_name>
      <joint_name>r_hip_pitch</joint_name>
      <joint_name>l_hip_roll</joint_name>
      <joint_name>r_hip_roll</joint_name>
      <joint_name>l_leg_extension</joint_name>
      <joint_name>r_leg_extension</joint_name>
      <joint_name>l_ankle_yaw</joint_name>
      <joint_name>r_ankle_yaw</joint_name>
      <joint_name>l_ankle_pitch</joint_name>
      <joint_name>r_ankle_pitch</joint_name>
      <joint_name>l_ankle_roll</joint_name>
      <joint_name>r_ankle_roll</joint_name>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_hip_pitch</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_hip_pitch</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_hip_roll</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_hip_roll</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_leg_extension</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>10</i_gain>
      <d_gain>10</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_leg_extension</joint_name>
      <p_gain>1000</p_gain>
      <i_gain>10</i_gain>
      <d_gain>10</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_ankle_yaw</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_ankle_yaw</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_ankle_pitch</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_ankle_pitch</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>l_ankle_roll</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>

    <plugin
      filename="libignition-gazebo-joint-position-controller-system"
      name="ignition::gazebo::systems::JointPositionController">
      <joint_name>r_ankle_roll</joint_name>
      <p_gain>100</p_gain>
      <i_gain>0</i_gain>
      <d_gain>0</d_gain>
    </plugin>
  </gazebo>
</robot>
